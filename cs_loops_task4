int sum = 0;
int divider1 = 3;
int divider2 = 5;
int maximumValue = 100;
Random rand = new Random();
int number = rand.Next(0, maximumValue);
Console.WriteLine(number);

for (int i = 1; i <= number; i++)
{
    if (i % divider1 == 0 || i % divider2 == 0)
    {
        Console.WriteLine(i);
        sum += i;
    }
}
